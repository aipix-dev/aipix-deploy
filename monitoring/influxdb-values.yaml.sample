adminUser:
  organization: "influxdata"
  bucket: "telegraf"
  user: "${INFLUX_USR}"
  retention_policy: "0s"
  ## Leave empty to generate a random password and token.
  ## Or fill any of these values to use fixed values.
  password: "${INFLUX_PSW}"
  token: "${INFLUX_TOKEN}"

persistence:
  enabled: true
  storageClass: "${RWO_STORAGE}"
  